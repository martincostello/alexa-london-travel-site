{
  "compileOnSave": true,
  "compilerOptions": {
    "esModuleInterop": true,
    "inlineSources": true,
    "lib": [
      "dom",
      "es5",
      "es2015.iterable",
      "scripthost"
    ],
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitThis": true,
    "outDir": "./wwwroot/assets/js",
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
