@{
    var analyticsId = Options.Analytics?.Google;
}
@if (!string.IsNullOrWhiteSpace(analyticsId))
{
    <script src="https://www.googletagmanager.com/gtag/js?id=@(analyticsId)" async></script>
    <script type="text/javascript" nonce="@Context.EnsureCspNonce()">
        var trackingId = document.querySelector('meta[name="google-analytics"]').getAttribute('content');
        if (trackingId) {
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());
            gtag('config', '@(analyticsId)');
        }
    </script>
